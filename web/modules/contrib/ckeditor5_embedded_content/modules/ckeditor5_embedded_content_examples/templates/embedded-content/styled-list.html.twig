<div class="styled-list" style="list-style: none">
  {% for item in items %}
    <div style="display: flex;margin-bottom:1rem;color:{{ style }}">
      <div style="margin-left: 2rem;margin-top:1rem;margin-right:.5rem"><span class="icon">>></span></div>
      <div class="content">
        {{ item }}
      </div>
    </div>
  {% endfor %}
</div>
